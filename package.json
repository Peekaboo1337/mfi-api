{
  "name": "mfi-api",
  "version": "0.0.1",
  "description": "A basic mPower, mFi Switch and mFi In-Wall Outlet HTTP API wrapper utilizing CURL and using ES-6 promises. Works on Linux, macOS and Windows.",
  "main": "./src/index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "test": "node ./test/index.js"
  },
  "keywords": ["mFi", "mPower", "mFi Switch", "ubiquiti"],
  "author": "Erek Röös",
  "license": "MIT",
  "dependencies": {
    "debug": "^3.1.0",
    "node-libcurl": "^1.2.0",
    "randomstring": "^1.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Peekaboo1337/mfi-api.git"
  },
  "bugs": {
    "url": "https://github.com/Peekaboo1337/mfi-api/issues"
  },
  "devDependencies": {
    "xo": "^0.18.2"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "ignores": [
      "test/*"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  }
}
